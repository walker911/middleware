<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.debug.middleware.model.mapper.SysLogMapper">

    <!-- 保存 -->
    <insert id="save" parameterType="com.debug.middleware.model.entity.SysLog">
        insert into sys_log (user_id, module, data, memo, create_time) values (#{userId}, #{module}, #{data}, #{memo}, #{createTime})
    </insert>
</mapper>